{
  "name": "mcp-blink-memory",
  "version": "2.0.0",
  "description": "MCP-compatible Knowledge Graph Memory System with TypeScript & JSON-RPC 2.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "mcp-blink-memory": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "nodemon --watch src --exec ts-node src/index.ts",
    "test": "jest --config jest.config.ts",
    "test:unit": "jest --config jest.config.ts --testPathPattern=unit",
    "test:integration": "jest --config jest.config.ts --testPathPattern=integration",
    "test:e2e": "jest --config jest.config.ts --testPathPattern=e2e",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "validate": "node validate-structure.js",
    "validate": "ts-node src/core/system/validator.ts",
    "validate:structure": "npm run validate -- --check-structure",
    "docs": "typedoc --out docs/api src/index.ts",
    "clean": "rm -rf dist coverage",
    "prepublishOnly": "npm run build && npm test",
    "release": "npm run build && npm test && npm run validate"
    "prepublishOnly": "npm run build && npm test"
  },
  "dependencies": {
    "json-rpc-2.0": "^1.0.0",
    "js-yaml": "^4.1.0",
    "winston": "^3.11.0",
    "zod": "^3.22.0",
    "js-yaml": "^4.1.0",
    "dotenv": "^16.3.0",
    "uuid": "^9.0.1",
    "axios": "^1.12.2"
    "axios": "^1.12.2",
    "lru-cache": "^11.2.1"
  },
  "devDependencies": {
    "typescript": "^5.0.0",
    "@types/node": "^20.0.0",
    "@types/js-yaml": "^4.0.0",
    "@types/uuid": "^9.0.0",
    "ts-node": "^10.9.0",
    "nodemon": "^3.0.0",
    "jest": "^29.0.0",
    "ts-jest": "^29.1.0",
    "@types/jest": "^29.0.0",
    "eslint": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "prettier": "^3.0.0"
    "prettier": "^3.0.0",
    "typedoc": "^0.25.0"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "json-rpc",
    "knowledge-graph",
    "memory-system",
    "typescript",
    "ai-agents"
  ],
  "author": "billlzzz10",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "files": [
    "dist",
    "src",
    "LICENSE",
    "README.md",
    "manifest.json"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/billlzzz10/MCP-Blink-momory.git"
  },
  "bugs": {
    "url": "https://github.com/billlzzz10/mcp-blink-memory/issues"
  },
  "homepage": "https://github.com/billlzzz10/mcp-blink-memory#readme"
}